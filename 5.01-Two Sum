//easy level
//HashMap
class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMao<Integer,Integer> m = new HashMap<Integer,Integer>;
        int[] res = new int[2];
        for(i=0;i<nums.length;i++){
          if(m.containskey(target-nums[i])){
            res[0] = i;
            res[1] = m.get(target-nums[i]);
            break;
          }
          m.put(nums[i],i);
         return res;
    }
}
